#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>
#define MAX 30
int main() 
{

    /* Enter your code here. Read input from STDIN. Print output to STDOUT */    
    char c;
    char s[10];
    char sen[MAX];
    scanf("%c",&c);
    scanf("%s",s);
    scanf("\n");
    scanf("%[^\n]%*c",sen);
    printf("%c\n",c);
    printf("%s\n",s);
    printf("%s",sen);
    
    return 0;
}
